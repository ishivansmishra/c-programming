int main()
{
    int *ptr;
    int n;
    printf("Enter The N : ");
    scanf("%d", &n);

    ptr = (int *)malloc(n * sizeof(int));
    for (int i = 0; i < n; i++)
    {
        printf("%d\n", ptr[i]);
    }

    free(ptr);
    ptr = (int *)malloc(2 * sizeof(int));
    for (int i = 0; i < 2; i++)
    {
        printf("%d\n", ptr[i]);
    }

    return 0;
}
