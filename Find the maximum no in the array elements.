#include <stdio.h>

// Find the maximum no in the array elements.

int main()
{
    int n;
    int arr[n];
    int max_element;

    printf("Enter The Size of Elements : ");
    scanf("%d", &n);

    printf("Enter The Elements in Array\n");
    for (int i = 0; i < n; i++)
    {
        scanf("%d", &arr[i]);
    }
    max_element = 0;

    for (int i = 1; i < n; i++)
    {
        if (max_element < arr[i])
        {
            max_element = arr[i];
        }
    }

    printf("The Maximum no elments in the Array is : %d", max_element);
    return 0;
}
