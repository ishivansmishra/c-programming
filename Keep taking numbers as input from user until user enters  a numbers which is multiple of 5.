#include<stdio.h>
//keep taking numbers as input from user until user enters a numbers which is multiple of 5.
int main (){
    int n;
   do{

  printf("enter n : ");
    scanf("%d",&n);
    printf("%d \n",n);
    if (n%5 == 0 ){
        break;
    }

   }while(1);
   printf("%d is multiple of 5");


    return 0;
}
