#include<stdio.h>
#include<math.h>
// Character is upper case or lower case.
int main (){
    char ch;
    printf("Enter Character : ");
    scanf("%s",&ch);

    if(ch >= 'A' && ch <= 'Z'){
        printf("UPPER CASE");
    }
    else if (ch >= 'a' && ch <= 'z'){
        printf("LOWER CASE ");
    }
    else {
        printf("INVALID");
    }
    return 0;
}
