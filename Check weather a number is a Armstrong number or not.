#include<stdio.h>
// Check weather a number is a Armstrong number or not.
int main (){
    int n;
    printf("Enter Number : ");
    scanf("%d",&n);

    int temp = n;
    int p = 0;

    while (n>0){
        int rem = n % 10;
        p = (p) + (rem * rem * rem);
        n = n / 10;
    } 
    if (temp = p){
        printf(" Yes,It is a Armstrong Number \n");
    } else {
        printf(" No,It is not Armstrong number \n");
    }

    return 0;
}
