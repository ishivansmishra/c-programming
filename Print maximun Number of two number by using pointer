#include<stdio.h>


// Print maximun Number of two number by using pointer.

int main () {
    int n1,n2;
    printf("Enter Number : ");
    scanf("%d",&n1);

     printf("Enter Number : ");
    scanf("%d",&n2);
    
    int *ptr1;
    int *ptr2;

    ptr1 = &n1;
    ptr2 = &n2;

    
    if(n1>n2){
        printf("Maximum No is : %d",*ptr1);

    }
    else{
        printf("Maximum No is : %d",*ptr2);
    }
    return 0;
}
