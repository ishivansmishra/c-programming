#include<stdio.h>
#include<math.h>

// Print sqroot of any range of numbers by using Loop.

int main (){
    double num = 0,sqroot = 0;
    printf("Enter Number : ");
    scanf("%lf",&num);

    for(int i=1;i<=num;i++){
        sqroot = sqrt(i);
        printf("Sqroot of %d is %.2f\n",i,sqroot);
    }

    return 0;

}
