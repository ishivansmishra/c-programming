#include<stdio.h>
#include<math.h>
//swapping two numbers.
int main(){
    int a,b;
    printf("Enter a :");
    scanf("%d",&a);

    printf("Enter b :");
    scanf("%d",&b);

    printf("before swapping : a = %d, b = %d \n",a,b);
    a = a+b;
    b = a-b;
    a = a-b;
    printf("after swapping : a =%d ,b =%d \n",a,b);

    return 0;

}
