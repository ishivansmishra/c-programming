#include <stdio.h>

// Create a program to print a Alphabet Triangle pattern.

int main()
{
    int rows;
    printf("Enter The Rows : ");
    scanf("%d", &rows);

    for (int i = 1; i <= rows; i++)
    {
        int a = 1;
        for (int j = 1; j <= i ; j++)
        {
            int d = a+64;
            char ch = (char)d;
            printf("%c",ch);
            a++;
           
        }
        printf("\n");
    }

    return 0;
}
