#include <stdio.h>

// Create a program to print a star triangle inveretd.

int main()
{
    int rows;
    printf("Enter The Rows : ");
    scanf("%d", &rows);

    for (int i = 0; i <= rows; i++)
    {
        for (int j = 0; j <= rows - i; j++)
        {
            printf("*");
        }
        printf("\n");
    }

    return 0;
}
