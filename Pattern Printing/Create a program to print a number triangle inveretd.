#include <stdio.h>

// Create a program to print a number triangle inveretd.

int main()
{
    int rows;
    printf("Enter The Rows : ");
    scanf("%d", &rows);

    for (int i = 1; i <= rows; i++)
    {
        for (int j = 1; j <= rows + 1 - i; j++)
        {
            printf("%d",j);
        }
        printf("\n");
    }

    return 0;
}
