#include <stdio.h>

// Create a program to print a odd  number tringle pattern.

int main()
{
    int rows;
    printf("Enter The Rows : ");
    scanf("%d", &rows);

    for (int i = 1; i <= 2*rows-1; i=i+2)
    {
        for (int j = 1; j <= i ; j= j+2)
        {
            printf("%d",j);
        }
        printf("\n");
    }

    return 0;
}
