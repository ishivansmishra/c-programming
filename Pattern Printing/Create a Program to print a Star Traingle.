#include <stdio.h>

// Create a Program to print a Star Traingle.

int main()
{
    int rows;
    printf("Enter The Rows : ");
    scanf("%d", &rows);

    for (int i = 1; i <= rows; i++)
    {
        for (int j = 1; j <= i; j++)
        {
            printf("*");
        }
        printf("\n");
    }
}
