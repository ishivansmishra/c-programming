#include<stdio.h>
#include<string.h>
// Make a new password by adding salting by using strings.
int main (){
  char password[100];
  printf("Enter the Password : ");
  gets(password);

  char salting[100];
  printf("Enter the Salting : ");
  gets(salting);

  printf("The New Password for company is : %s",strcat(password,salting));

  return 0;
}
