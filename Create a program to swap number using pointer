#include<stdio.h>

void swap(int *a,int *b){
    int temp;
    temp = *a;
    *a = *b;
    *b = temp;
}


int main (){
    int a;
    printf("Enter The Value of a : ");
    scanf("%d",&a);

    int b;
    printf("Enter The Value of b : ");
    scanf("%d",&b);

    printf("Before swapping\n The Value of a is : %d\n The Value of B is : %d\n",a,b);

    swap(&a,&b);

    printf("After swapping\n The Value of a is : %d\n The Value of B is : %d",a,b);


    return 0;
}
