#include<stdio.h>
// Print pattern by numbers in loop.
int main (){
    int i,j,rows;
    printf("Enter number : ");
    scanf("%d",&rows);

    for(int i = 1;i<=rows;i++){

        for(int j = 1;j<=i;j++)
            printf("%d",j);
            printf(" \n");
                
    }
    return 0;
}
