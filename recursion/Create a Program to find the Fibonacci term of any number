#include <stdio.h>

int fib(int n)
{
    if (n == 0 || n == 1)
    {
        return 1;
    }

    return fib(n - 1) + fib(n - 2);
}

int main()
{
    int n;
    printf("Enter Tthe Value Of No : ");
    scanf("%d", &n);

    printf("The Fibonacci Sequence of %d term is : %d", n, fib(n));

    return 0;
}
