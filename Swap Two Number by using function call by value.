#include<stdio.h>
#include<stdlib.h>
#include<math.h>

// Swap Two Number by using function call by value.

void swap(int a,int b);

int main () {
  int x;
  int y;
  printf("Enter The value of a : ");
  scanf("%d",&x);

  printf("Enter The Value Of B : ");
  scanf("%d",&y);

  swap(x,y);
  printf("The swapping number of X = %d & Y = %d\n",x,y);



  return 0;
}
//  call by value
void swap(int a,int b){
  int t = a;
  a = b;
  b = t;
  printf("The swapping number of A = %d & B= %d\n",a,b);
}
