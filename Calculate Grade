#include<stdio.h>
//Calculate Grade
int main (){
    int  marks;
    printf("enter marks :  ");
    scanf("%d",&marks);

    if(marks >= 90 ){
        printf("A++ \n");
        printf("Pass \n");
    }
    else if (marks >= 75) {
        printf("A \n");
        printf("Pass \n");
    }
    else if (marks >= 55){
        printf("B \n");
        printf("Pass \n");
    }
    else if (marks >= 45){
        printf("C \n");
        printf("Pass \n");
    }
    else if (marks >= 33){
        printf("D \n");
        printf("Pass \n");
    }
    else {
        printf("FAIL \n");
        printf("Better luck next time \n");
    }
    printf("THANK YOU \n");

    return 0;
}
