#include<stdio.h>
//Create a Program to print a pattern of recatangle with stars.
int main (){
int r;
printf("Enter The no of rows : ");
scanf("%d",&r);

int c;
printf("Enter The no of column : ");
scanf("%d",&c);

for(int i = 1;i<=r;i++){
    for(int i = 1;i<=c;i++){
        printf("*");
    }
    printf("\n");
}

    return 0;
}
