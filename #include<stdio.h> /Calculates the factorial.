#include<stdio.h>
//calculates the factorial.
int main (){
    int num;
    unsigned long long factorial = 1;

    printf("Enter a postive integer :  ");
    scanf("%d",&num);

    if (num < 0){
        printf("factorial is not defined");
    } else {
        for (int i = 1; i <= num ; ++i) {
            factorial *= i;
        }
        printf("Factorial of %d is %llu \n",num,factorial);
    }


    return 0;
}
