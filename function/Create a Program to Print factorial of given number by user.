#include <stdio.h>

int factorial(int a)
{

    if (a == 0 || a == 1)
    {
        return 1;
    }

    int fact = 1;
    for (int i = 1; i <= a; i++)
    {
        fact = fact * i;
    }

    return fact;
}
int main()
{
    int a;
    printf("Enter Positive Number : ");
    scanf("%d", &a);

    printf("Factorial of %d is : %d", a, factorial(a));

    return 0;
}
