#include<stdio.h>
int sum(int a,int b);
int main () {
    int a,b;
    printf("enter the value of a : ");
    scanf("%d",&a);
    printf("enter the value of b : ");
    scanf("%d",&b);

    int x = sum(a,b);
    printf("sum is : %d",x);




    return 0;
}
int sum (int a, int b){
   return a + b;

}
