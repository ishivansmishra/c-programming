#include<stdio.h>
#include<string.h>

// comparision of two user defined strings.



int main (){
    char str1[100],str2[100];

    printf("Enter the First string : ");
    scanf("%s",str1);

       printf("Enter the Second string : ");
    scanf("%s",str2);

    printf("comapre between two strings is : %d",strcmp(str1,str2));

    return 0;
}
