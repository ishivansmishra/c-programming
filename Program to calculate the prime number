#include<stdio.h>
#include<math.h>

// Program to calculate the prime number.

int main () 
{
     int i,n,limit,prime;
     printf("Enter Number : ");
     scanf("%d",&limit);

     printf("\n Prime numbers less than [%d] are :: \n\n",limit);

     for(n=2;n<limit;n++)
     {
        prime = 1;
        for(i=2;i<n;i++)
        
            if(n%i==0)
            {
                prime = 0;
                break;
            }
            if(prime)
            printf(" %d",n);
        

     }
     printf("\n");


    return 0;
}
