#include<stdio.h>
// Vowels and constant checkers
int main (){
    char ch;
    printf("Enter Alphabets : ");
    scanf("%c",&ch);

   if ("ch >= 'a" && ch <= 'z' || ch >= 'A' && ch <= 'Z')
   {
    if(ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u'
   || ch =='A' || ch == 'E' || ch == 'I' || ch == 'O' || ch == 'U'){

    printf("%c is a vowels");
    } 
    else if (ch >= 'a' && ch <= 'z' || ch >='A' && ch <= 'Z'){
        printf("%c is a constant");
    }
    else {
        printf("Invalid : enter a alphabet");
    }
   
   } 
   

   
    return 0;
}
