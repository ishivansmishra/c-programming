#include<stdio.h>
// Create a program to check file exist or not.
int main (){
    FILE *fptr;

    fptr = fopen("he.txt","r");
    if (fptr == NULL){
        printf("The File that you have entered is exist ");
    }
    else {
        printf("The File that you have enteres is not exist");
    }


    fclose(fptr);


    return 0;
}
